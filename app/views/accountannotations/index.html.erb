<p id="notice"><%= notice %></p>

<h1>Accountannotations</h1>

<table>
  <thead>
    <tr>
      <th>Annotation</th>
      <th>Account</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accountannotations.each do |accountannotation| %>
      <tr>
        <td><%= accountannotation.annotation_id %></td>
        <td><%= accountannotation.account_id %></td>
        <td><%= link_to 'Show', accountannotation %></td>
        <td><%= link_to 'Edit', edit_accountannotation_path(accountannotation) %></td>
        <td><%= link_to 'Destroy', accountannotation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Accountannotation', new_accountannotation_path %>
